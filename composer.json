{
    "name": "webit/message-bus",
    "type": "library",
    "keywords": ["message bus", "message queue"],
    "authors": [{
        "name": "Daniel Bojdo",
        "email": "daniel.bojdo@web-it.eu"
    }],
    "license": "MIT",
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\MessageBus\\": "src/"
        }
    },
    "require-dev": {
        "psr/log": "^1.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload-dev": {
        "psr-4": {
          "Webit\\MessageBus\\": "tests/"
        }
    },
    "suggest": {
        "psr/log": "To use logging publisher, consumer and exception handlers."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
